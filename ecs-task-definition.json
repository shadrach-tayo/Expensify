{
    "family": "expensify",
    "executionRoleArn": "arn:aws:iam::751808579985:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "expensify",
            "image": "751808579985.dkr.ecr.us-east-1.amazonaws.com/trans-app:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}